{
  "extends": "eslint-config-airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "comma-dangle": 0,
    "dot-notation": 0,
    "func-names": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "vars-on-top": 0
  }
}
